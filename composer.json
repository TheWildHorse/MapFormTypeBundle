{
    "name": "igor-rinkovec/map-form-type-bundle",
    "type": "symfony-bundle",
    "description": "Generic Map FormType for Symfony 3",
    "keywords": ["symfony", "map", "maps", "form", "formtype", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "webber",
            "email": "webber.nl@gmail.com"
        },
        {
            "name": "j63",
            "email": "jwoudstr@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/security-bundle": "^4.0",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|~5.0",
        "symfony/console": "^3.3",
        "symfony/phpunit-bridge": "^3.3"
    },
    "extra": {
        "scripts-dev": {
            "post-install-cmd": [
                "yarn && bower install"
            ],
            "post-update-cmd": [
                "yarn && bower install"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "CuriousInc\\MapFormTypeBundle\\": ""
        },
        "exclude-from-classmap": [
           "Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "CuriousInc\\MapFormTypeBundle\\Tests\\": "Tests/" }
    },
    "config": {
        "sort-packages": true
    }
}
